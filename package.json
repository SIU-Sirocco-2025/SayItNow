{
  "name": "eco-track",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:all",
    "start:all": "concurrently -k -n web,airvisual,openaq -c green,cyan,magenta \"npm run start:web\" \"npm run start:airvisual\" \"npm run start:openaq\"",
    "start:web": "node index.js",
    "start:airvisual": "node scripts/fetch-and-save.js",
    "start:openaq": "node scripts/fetch-openaq-hours.js",
    "dev": "npm run dev:all",
    "dev:all": "concurrently -k -n web,airvisual,openaq -c yellow,cyan,magenta \"npm run dev:web\" \"npm run dev:airvisual\" \"npm run dev:openaq\"",
    "dev:web": "nodemon index.js",
    "dev:airvisual": "nodemon scripts/fetch-and-save.js",
    "dev:openaq": "nodemon scripts/fetch-openaq-hours.js",
    "worker": "npm run worker:all",
    "worker:all": "concurrently -k -n airvisual,openaq -c cyan,magenta \"npm run start:airvisual\" \"npm run start:openaq\"",
    "worker:dev": "concurrently -k -n airvisual,openaq -c cyan,magenta \"npm run dev:airvisual\" \"npm run dev:openaq\"",
    "create-admin": "node scripts/create-admin.js"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/eco-track.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/eco-track/issues"
  },
  "homepage": "https://github.com/your-org/eco-track#readme",
  "description": "",
  "dependencies": {
    "axios": "^1.13.2",
    "body-parser": "^2.2.0",
    "cloudinary": "^2.8.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-flash": "^0.0.2",
    "express-session": "^1.18.2",
    "fs-extra": "^11.3.2",
    "json2csv": "^6.0.0-alpha.2",
    "md5": "^2.3.0",
    "method-override": "^3.0.0",
    "moment": "^2.30.1",
    "mongoose": "^8.19.2",
    "mongoose-slug-updater": "^3.3.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "path": "^0.12.7",
    "pug": "^3.0.3",
    "qs": "^6.14.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11"
  }
}
