{
  "name": "eco-track",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:all",
    "start:all": "concurrently -k -n web,worker -c green,cyan \"npm run start:web\" \"npm run start:worker\"",
    "start:web": "node index.js",
    "start:worker": "node scripts/fetch-and-save.js",
    "cron:fetch": "node scripts/fetch-and-save.js",
    "dev": "concurrently -k -n web,worker -c yellow,magenta \"nodemon index.js\" \"nodemon scripts/fetch-and-save.js\"",
    "dev:web": "nodemon index.js",
    "dev:worker": "nodemon scripts/fetch-and-save.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
